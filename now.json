{
  "name": "welcome-ui",
  "version": 2,
  "alias": ["welcome-ui.now.sh"],
  "builds": [
    { "src": "package.json", "use": "@now/static-build", "config": { "distDir": "docs" } }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/assets.json", "dest": "/assets.json" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
